{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    // "strict": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
  },
  // "include": ["**/*.ts"],
  "exclude": ["dist", "node_modules"]
}
